{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:*",
        "s3:ListBucket",
        "s3:CreateBucket",
        "s3:DeleteBucket",
        "s3:ForceDeleteBucket",

        "s3:GetBucketLocation",
        "s3:ListAllMyBuckets",
        "s3:DeleteObject",
        "s3:GetObject",
        "s3:PutObject"

      ],
      "Resource": [
        "arn:aws:s3:::srm-defualt/*"
      ]
    }
  ]
}